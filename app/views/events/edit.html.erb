<div class="edit_event_form">
<%= form_for(@event) do |f| %>
    <%= f.label :date_object %>
    <%= f.date_select :date_object%><br>
    <%= f.label :title %>
    <%= f.text_field :title %><br>
    <%= f.label :description %>
    <%= f.text_field :description %><br>
    <%= f.label :start_time %>
    <%= f.text_field :start_time %><br>
    <%= f.label :end_time %>
    <%= f.text_field :end_time %><br>

    <%=f.submit "Create Event"%>
<% end %>
</div>